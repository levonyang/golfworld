/**
 * 处理数字，如果少于2位则前面加0（用于时间格式化）
 */
var formatNumber = function (n) {
    n = n.toString()
    return n[1] ? n : '0' + n
}

function formatDate(date) {
    if (null == date | date == undefined ) return ''
    let datas =date.split('-')
    let dateTimeStr = datas[1] + '月' + datas[2] + '号'
    return dateTimeStr
}

module.exports = {
    formatDate: formatDate
}