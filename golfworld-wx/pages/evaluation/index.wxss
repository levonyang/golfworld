.header-less-content {
    margin-top: 150rpx;
    width: 100%;
    height: 100%;
}

.banner {
    border-radius: 30rpx
}

.banner-item {
    border-radius: 30rpx !important;
}

.title-name {
    font-size: 32rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: rgba(0, 0, 0, 1);
    line-height: 44rpx;
}

.title-link-button {
    float: right;
    width: 112rpx;
    height: 38rpx;
    text-align: center;
    align-items: center;
    background: rgba(255, 255, 255, 1);
    border-radius: 19rpx;
    border: 2px solid rgba(207, 207, 207, 1);
    font-size: 16rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: rgba(207, 207, 207, 1);
    line-height: 33rpx;
}
